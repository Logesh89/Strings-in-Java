import java.util.*;
public class Main
{
	public static void main(String[] args) {
	    Scanner sc=new Scanner(System.in);
	    String x=sc.nextLine();
	    char y[]=x.toCharArray();
	    int size=y.length;
	    char a[]=new char[size];
	    int i=0;
		while(i!=size)
		{
		    a[size-i-1]=y[i];
		    ++i;
		}
		System.out.println(a);
	}
}
